{
    "globals": {},
    "extends": [
        "/home/leon/.eslintrc",
        "plugin:react/recommended"
    ],
    "plugins": [
        "import",
        "react",
        "@babel"
    ],
    "rules": {
        "import/no-unresolved": 0,
        "import/named": 2,
        "import/namespace": 2,
        "import/default": 2,
        "import/export": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "jsx": true,
            "classes": true
        }
    },
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}
